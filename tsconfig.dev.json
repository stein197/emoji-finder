{
	"compilerOptions": {
		"target": "ESNext",
		"jsx": "react",
		"baseUrl": "src",
		"noImplicitAny": false,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"sourceMap": true,
		"inlineSources": true,
		"importsNotUsedAsValues": "remove",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true
	}
}
